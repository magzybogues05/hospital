<div class="modal fade" id="patientsignupModal" tabindex="-1" role="dialog" aria-labelledby="signupModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document" style="max-width: 100%; margin: 0px; padding:0px;">
      <div class="modal-content">
        <div class="modal-header">
            <div class="logintitle">
                <h5 class="modal-title" id="signupModalLabel">Sign up as Patient</h5>
            </div>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
            <form>
                <div class="patientform">
                    <div class="leftpatientform">
                        <div class="form-group">
                            <!-- <label for="signUpInputEmail1">Email address</label> -->
                            <input type="text" class="form-control" name="" id="patientName" placeholder="Enter Name">
                        </div>
                        <div class="form-group">
                            <!-- <label for="signUpInputEmail1">Email address</label> -->
                            <input type="text" class="form-control" name="" id="patientmobilenumber" placeholder="Enter Mobile Number">
                        </div>
                        <div class="form-group">
                            <!-- <label for="signUpInputEmail1">Email address</label> -->
                            <input type="text" class="form-control" name="" id="patientdateofbirth" placeholder="Enter Date of Birth">
                        </div>
                        <div class="form-group">
                            <!-- <label for="signUpInputEmail1">Email address</label> -->
                            <input type="text" class="form-control" name="" id="patientgender" placeholder="Enter Gender">
                        </div>
                        <div class="form-group">
                            <!-- <label for="signUpInputEmail1">Email address</label> -->
                            <input type="text" class="form-control" name="" id="patientpincode" placeholder="Enter Pin Code">
                        </div>
                        <div class="form-group">
                            <!-- <label for="signUpInputEmail1">Email address</label> -->
                            <input type="text" class="form-control" name="" id="patientstate" placeholder="Enter State">
                        </div>
                    </div>
                    <div class="rightpatientform">
                        <div class="form-group">
                            <!-- <label for="signUpInputEmail1">Email address</label> -->
                            <input type="text" class="form-control" name="" id="patientcity" placeholder="Enter city">
                        </div>
                        <div class="form-group">
                            <!-- <label for="signUpInputEmail1">Email address</label> -->
                            <input type="text" class="form-control" name="" id="patientcountry" placeholder="Enter country">
                        </div>
                        <div class="form-group">
                        <!-- <label for="signUpInputEmail1">Email address</label> -->
                        <input type="email" class="form-control" id="signUpInputEmail1" aria-describedby="emailHelp" placeholder="Enter email">
                        </div>
                        <div class="form-group">
                        <!-- <label for="signUpInputPassword1">Password</label> -->
                        <input type="password" class="form-control" id="signUpInputPassword1" placeholder="Enter Password">
                        
                        </div>
                        <div class="form-group">
                            <input type="password" class="form-control" id="signUpconfirmPassword1" placeholder="Confirm Password">
                            <small id="emailHelp" class="form-text text-muted">*Password must have length atleast six</small>
                        </div>
                    </div>
                </div>


                <div style="display: flex; justify-content: center; align-items: center;">
                    <button type="submit" class="btn btn-primary SignupSubmitbtn">SignUp</button>
                </div>
              </form>
        </div>
      </div>
    </div>
</div>

<div class="modal fade" id="doctorsignupModal" tabindex="-1" role="dialog" aria-labelledby="signupModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document" style="max-width: 100%; margin: 0px; padding:0px;">
      <div class="modal-content">
        <div class="modal-header">
            <div class="logintitle">
                <h5 class="modal-title" id="signupModalLabel">Sign up as Doctor</h5>
            </div>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
            <form action="/signup" method="POST">
                <div class="patientform">
                    <div class="leftpatientform">
                        <div class="form-group">
                            <!-- <label for="signUpInputEmail1">Email address</label> -->
                            <input type="text" class="form-control" name="docName" id="doctorName" placeholder="Enter Name">
                        </div>
                        <div class="form-group">
                            <!-- <label for="signUpInputEmail1">Email address</label> -->
                            <!-- <label for="gender">Gender:</label> -->
                            <select name="docgender" id="doctorgender" class="form-control">
                            <option value="">Select Gender</option>
                            <option value="female">Female</option>
                            <option value="male">Male</option>
                            <option value="non-binary">Non-Binary</option>
                            <option value="other">Other</option>
                            <option value="Prefer not to answer">Perfer not to Answer</option>
                            </select>
                            <!-- <input type="dropbox" class="form-control" name="docgender" id="doctorgender" placeholder="Enter Gender"> -->
                        </div>
                        <div class="form-group">
                            <!-- <label for="signUpInputEmail1">Email address</label> -->
                            <input type="text" class="form-control" name="docmobilenumber" id="doctormobilenumber" placeholder="Enter Mobile Number">
                        </div>
                        <div class="form-group">
                            <!-- <label for="signUpInputEmail1">Email address</label> -->
                            <input type="text" class="form-control" name="docdegree" id="doctordegree" placeholder="Enter Highest Qualification">
                        </div>
                        <div class="form-group">
                            <!-- <label for="signUpInputEmail1">Email address</label> -->
                            <input type="text" class="form-control" name="docExperience" id="doctorExperience" placeholder="Enter Experience (in years)">
                        </div>
                        
                        <div class="form-group">
                            <!-- <label for="signUpInputEmail1">Email address</label> -->
                            <input type="text" class="form-control" name="docspeciality" id="doctorspeciality" placeholder="Enter Speciality">
                        </div>
                       
                    </div>
                    <div class="rightpatientform">
                        <div class="form-group">
                            <!-- <label for="signUpInputEmail1">Email address</label> -->
                            <input type="text" class="form-control" name="docNationality" id="doctorNationality" placeholder="Enter Nationality">
                        </div>
                        <div class="form-group">
                            <!-- <label for="signUpInputEmail1">Email address</label> -->
                            <input type="text" class="form-control" name="doccity" id="doctorcity" placeholder="Enter city">
                        </div>
                        <div class="form-group">
                            <!-- <label for="signUpInputEmail1">Email address</label> -->
                            <input type="text" class="form-control" name="docclinic" id="doctorclinic" placeholder="Enter Hospital/Clinic">
                        </div>
                        
                        <div class="form-group">
                        <!-- <label for="signUpInputEmail1">Email address</label> -->
                        <input type="email" class="form-control" name="docsignUpInputEmail1" id="doctorsignUpInputEmail1" aria-describedby="emailHelp" placeholder="Enter email">
                        </div>
                        <div class="form-group">
                        <!-- <label for="signUpInputPassword1">Password</label> -->
                        <input type="password" class="form-control" name="docsignUpInputPassword1" id="doctorsignUpInputPassword1" placeholder="Enter Password">
                        
                        </div>
                        <div class="form-group">
                            <input type="password" class="form-control" name="docsignUpconfirmPassword1" id="doctorsignUpconfirmPassword1" placeholder="Confirm Password">
                            <small id="emailHelp" class="form-text text-muted">*Password must have length atleast six</small>
                        </div>
                    </div>
                </div>

                <div style="display: flex; justify-content: center; align-items: center;">

                    <button type="submit" class="btn btn-primary SignupSubmitbtn">SignUp</button>
                </div>
              </form>
        </div>
      </div>
    </div>
</div>



